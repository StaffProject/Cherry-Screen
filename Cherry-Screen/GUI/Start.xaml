<Window x:Class="Cherry_Screen.GUI.Start"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Cherry_Screen.GUI"
        Title="Start" Height="1080" Width="1920">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="930"></RowDefinition>
            <RowDefinition Height="150"></RowDefinition>
        </Grid.RowDefinitions>

        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="960"></ColumnDefinition>
            <ColumnDefinition Width="960"></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <local:OptionsBar Grid.Row="1" Grid.Column="0"></local:OptionsBar>
        <local:ClockBar Grid.Row="1" Grid.Column="1"></local:ClockBar>
        
        
        <WebBrowser Name="news" HorizontalAlignment="Left" Height="380" Margin="80,45,0,0" VerticalAlignment="Top" Width="770"/>
        <WebBrowser Name="replacement" HorizontalAlignment="Left" Height="380" Margin="80,480,0,0" VerticalAlignment="Top" Width="770"/>


    </Grid>

</Window>
